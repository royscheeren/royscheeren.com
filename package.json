{
  "name": "@royscheeren/root",
  "version": "1.0.0",
  "description": "Mono Repo for royscheeren projects",
  "main": "index.js",
  "scripts": {
    "ls": "lerna ls",
    "bootstrap": "lerna bootstrap",
    "lerna:update": "lerna update",
    "publish": "lerna publish",
    "create:package": "lerna create",
    "test": "jest",
    "_git:pre-commit:all": "lerna run _git:pre-commit --stream --concurrency=1 -- --colors",
    "_git:pre-push:all": "lerna run _git:pre-push --stream --concurrency=1 -- --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/royscheeren/royscheeren.com.git"
  },
  "keywords": [
    "Personal"
  ],
  "author": "@royscheeren",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/royscheeren/royscheeren.com/issues"
  },
  "homepage": "https://github.com/royscheeren/royscheeren.com#readme",
  "devDependencies": {
    "husky": "^4.3.8",
    "lerna": "^3.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run _git:pre-commit:all",
      "pre-push": "npm run _git:pre-push:all"
    }
  },
  "private": true,
  "dependencies": {}
}
